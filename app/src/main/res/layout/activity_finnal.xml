<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<!--头部导航栏--><LinearLayout
        android:layout_width="500dp"
        android:layout_height="120dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="20dp"
        android:orientation="horizontal"
    android:id="@+id/docker">
    <LinearLayout
        android:layout_marginLeft="50px"
        android:layout_width="100dp"
        android:layout_marginTop="20dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/space_station"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="space_station" />

    </LinearLayout>
    <LinearLayout
        android:layout_marginLeft="50px"
        android:layout_width="100dp"
        android:layout_marginTop="20dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/rover_icon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="18dp"
            android:text="rovers" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginLeft="50px"
        android:layout_width="100dp"
        android:layout_marginTop="20dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/rocket_icon"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="25px"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="15dp"
            android:text="flights" />
    </LinearLayout>

    </LinearLayout>
    <TextView
        android:layout_marginLeft="80dp"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:background="@color/gray"
        android:id="@+id/DCA"
        android:text="DCA"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/docker"
        />
    <TextView
        android:layout_marginLeft="20dp"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:text="MARS"
        android:gravity="center"
        android:id="@+id/MARS"
        app:layout_constraintLeft_toRightOf="@+id/DCA"
        app:layout_constraintTop_toBottomOf="@id/docker"
        android:background="@color/gray"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="@id/MARS"
        app:layout_constraintBottom_toBottomOf="@id/MARS"
        app:layout_constraintLeft_toLeftOf="@id/DCA"
        app:layout_constraintRight_toRightOf="@id/MARS"
        android:id="@+id/double_arrows"
        />
    <TextView
        app:layout_constraintRight_toRightOf="@+id/DCA"
        app:layout_constraintTop_toBottomOf="@+id/DCA"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:text="oneway"
        android:id="@+id/oneway"
        android:background="@color/orange"/>
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="@+id/oneway"
        app:layout_constraintTop_toBottomOf="@+id/oneway"
        android:background="@color/orange"
        android:text="orange"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/rocket_icon1"
        app:layout_constraintTop_toBottomOf="@id/DCA"
        app:layout_constraintLeft_toLeftOf="@id/DCA"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="30dp"/>
    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintTop_toTopOf="@id/rocket_icon1"
        app:layout_constraintBottom_toBottomOf="@id/rocket_icon1"
        app:layout_constraintLeft_toRightOf="@id/rocket_icon1"
        android:id="@+id/galaxy"
        android:layout_marginLeft="15dp"/>
<TextView
    android:layout_width="match_parent"
    android:layout_height="50dp"
    app:layout_constraintBottom_toBottomOf="parent"
    android:text="depart"
    android:background="@color/green"/>









